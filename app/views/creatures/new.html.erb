<!-- Form Goes Here -->

<%= form_for :creature, url: "/creatures", method: "post" do |fromform| %>
<div class="col m4">
  <h5>Creature Name:</h5>
  <%=fromform.text_field :name%>
  <h5>Creature Description:</h5>
  <%=fromform.text_area :description%>
  <%=fromform.submit "Add Creature"%>
</div>
<% end %>
